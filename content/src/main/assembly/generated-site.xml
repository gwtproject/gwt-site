<assembly>
  <id>generated-site</id>
  <includeBaseDirectory>false</includeBaseDirectory>
  <formats>
    <format>zip</format>
  </formats>
  <fileSets>
    <fileSet>
      <directory>target/generated-site</directory>
      <includes>
        <include>**/*.html</include>
        <include>**/*.css</include>
        <include>**/*.js</include>
        <include>**/*.png</include>
        <include>**/*.jpg</include>
        <include>**/*.jpeg</include>
        <include>**/*.gif</include>
        <include>**/*.dtd</include>
        <include>**/package-list</include>
        <include>**/*.eot</include>
        <include>**/*.svg</include>
        <include>**/*.ttf</include>
        <include>**/*.woff</include>
        <include>**/*.woff2</include>
      </includes>
      <outputDirectory></outputDirectory>
    </fileSet>
  </fileSets>
</assembly>
